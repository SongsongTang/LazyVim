{
    "Table": {
        "prefix": "tab",
        "body": [
            "\\begin{table}[${1:!ht}]",
            "    \\centering",
            "    \\caption{${2:caption}}",
            "    \\label{tab:${3:label}}",
            "    \\begin{tabular}{${4:c c}}",
            "    ${5:\\toprule}",
            "    $6    &    $7    \\\\\\",
            "    ${8:\\midrule}",
            "    $9    &    $10    \\\\\\",
            "    ${11:\\bottomrule}",
            "    \\end{tabular}",
            "\\end{table}"
        ],
        "description": "table env"
    },
    "Single Figure": {
        "prefix": "fig",
        "body": [
            "\\begin{figure}[${1:htbp}]",
            "    \\centering",
            "    \\includegraphics[width=$3\\textwidth]{$2}",
            "    \\caption{${4:caption}}",
            "    \\label{fig:${5:label}}",
            "\\end{figure}$0"
        ],
        "description": "figures env"
    },
    "Double Figure": {
        "prefix": "fig2",
        "body": [
            "\\begin{figure}[${1:htbp}]",
            "    \\centering",
            "    \\begin{subfigure}[b]{${2:0.45}\\textwidth}",
            "        \\includegraphics[width=\\textwidth]{$3}",
            "        \\caption{${4:caption}}",
            "        \\label{fig:${5:label}}",
            "    \\end{subfigure}",
            "    \\hfill",
            "    \\begin{subfigure}[b]{${6:0.45}\\textwidth}",
            "        \\includegraphics[width=\\textwidth]{$7}",
            "        \\caption{${8:caption}}",
            "        \\label{fig:${9:label}}",
            "    \\end{subfigure}",
            "    \\caption{${10:caption}}",
            "    \\label{fig:${11:label}}",
            "\\end{figure}$0"
        ],
        "description": "figures env"
    },
    "Triple Figure": {
